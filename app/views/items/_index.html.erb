<ul>
<% if item.item_users.find_by(user_id: current_user.id) %>
    <li><%= link_to item.title, item_path(item) %> | <%= item.description %></li>
<% else %>
    <li><%= link_to item.title, item_path(item) %> | <%= item.description %>  <%= button_to "Reserve", reserved_items_path, params: {user_id: current_user.id, item_id: item.id}, :class => "button is-small" %></li>
<% end %>
</ul>


